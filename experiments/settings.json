{
    "wd": "/tmp/exps",
    "project": {
        "git": {
            "branch": "effect-handlers-compilation",
            "url": "https://github.com/links-lang/links.git"
        },
        "directory": "$LINKS_SRC_DIR"
    },
    "compilers": [
        {
            "name": "linksc",
            "interactive": false,
            "comment": "The Links compiler",
            "exec": "$LINKS_SRC_DIR/links",
            "defaultFlags": "-c --native --verbose"
        },
        {
            "name": "linksi",
            "interactive": true,
            "comment": "The Links interpreter",
            "exec": "$LINKS_SRC_DIR/links",
            "defaultFlags": "--config=$LINKS_SRC_DIR/measure.config"
        },
        {
            "name": "ocamlopt",
            "interactive": false,
            "comment": "The OCaml optimising compiler",
            "exec": "ocamlopt",
            "defaultFlags": "-dlambda -dclambda -dcmm"
        }
    ],
    "repetitions": 5,
    "experiments": [
        { "directory": "$LINKS_SRC_DIR/programs",
          "programs": [
              {
                  "comment": "8 queens program",
                  "filename": "queens8_minimal.links",
                  "checksum": "",
                  "compilers": [
                      {"id": 0},
                      {"id": 1}
                  ]
              },
              {
                  "comment": "20 queens program",
                  "filename": "queens20_minimal.links",
                  "checksum": "",
                  "compilers": [
                      {"id": 0},
                      {"id": 1}
                  ]
              }
          ]
        }
    ]
}
